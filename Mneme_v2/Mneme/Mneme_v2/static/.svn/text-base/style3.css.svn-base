/* 2ND LEVEL DROP-DOWN MENU */
#menu ul#nav li > ul#nav2 li ul { 
	             display:none; 
}
#menu ul#nav li > ul#nav2 li:hover > ul { 
		padding:10px;
		margin:25px;
		border:0px;
		position:absolute;
		width:225px;
		z-index:200;
                display:block; 
                position:absolute;  
		top:30px;
		background-color:#333333 ;

}
#menu ul#nav li > ul#nav2 li:hover ul li { 
		margin:0px; 
		padding:0px 0px; 
		float:none; 
		list-style:none; 		
		background:url('images/sub_sep.gif') bottom left no-repeat;
		width:225px; 
		position:relative; 
}
#menu ul#nav li > ul#nav2 li:hover ul li a { 
		display:block; 
                background:none;
}
#menu ul#nav li > ul#nav2 li:hover ul li a:hover { 
		background: #008B8B url('images/hover_sub.gif') center left no-repeat;
		border:0px;
		color:#fff;
		text-decoration:none;

}

#menu p{
		clear:left;
		}	

