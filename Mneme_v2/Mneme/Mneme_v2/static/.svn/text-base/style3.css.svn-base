		text-transform:uppercase;
	}
#menu ul#nav li a.menu:hover
	{
		height:22px;
		float: left;
		margin:0px;
		padding:5px 11px 0 11px;
		font-family: "Trebuchet MS";
		font-size:14px;
		font-weight:bold;
		color:#FFF;
		text-align:center;
		text-decoration:none;
		text-transform:uppercase;
                background-color:#008B8B;
		background-image: url(images/menuover.jpg);
		background-repeat:no-repeat;
		background-position:center bottom;
	}
/*-------1st level drop down menu-----*/
#menu ul#nav li ul#nav2{
		background:#333333;
		display:none;
		height:auto;
		padding:10px;
		margin:25px;
		border:0px;
		position:absolute;
		width:225px;
		z-index:200;
		/*top:1em;*/
		/*left:0;*/
		}
#menu ul#nav li:hover ul#nav2{
		display:block;
		
		}
#menu li li {
		background:url('images/sub_sep.gif') bottom left no-repeat;
		display:block;
		float:none;
		margin:0px;
		padding:0px;
		width:225px;
                color:#FFF; 
                text-align:center;
                text-transform:uppercase;  
		}
#menu li:hover li a{
		background:none;
		
		}
#menu ul#nav li ul#nav2 a{
		display:block;
		height:22px;
		font-size:14px;
		font-style:normal;
                color:#FFF;
		margin:0px;
		padding:0px 10px 0px 15px;
		text-align:left;
		}

#menu li ul#nav2 a:hover, #menu li ul#nav2 li:hover a{
			background:#008B8B url('images/hover_sub.gif') center left no-repeat;
			border:0px;
			color:#fff;
			text-decoration:none;
			}

