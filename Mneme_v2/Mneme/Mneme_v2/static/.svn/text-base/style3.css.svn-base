        /* Style for overlay and box */

.overlay{
            background:transparent url('images/overlay.jpg') repeat top left;
            position:fixed;
            top:0px;
            bottom:0px;
            left:0px;
            right:0px;
            z-index:100;
        }
.box{
            position:fixed;
            top:-400px;
            left:30%;
            right:30%; 
            height: 120px;           
            background-color:#282828;
            color:#FFF;
            font-size:14px;
            padding:20px;
            border:2px solid #ccc;
            -moz-border-radius: 20px;
            -webkit-border-radius:20px;
            -khtml-border-radius:20px;
            -moz-box-shadow: 0 1px 5px #333;
            -webkit-box-shadow: 0 1px 5px #333;
            z-index:101;
        }
.displaybox{
            position:fixed;
            top:300px;
            left:30%;
            right:30%; 
            height: 120px;           
            background-color:#282828;
            color:#FFF;
            padding:20px;
            border:2px solid #ccc;
            -moz-border-radius: 20px;
            -webkit-border-radius:20px;
            -khtml-border-radius:20px;
            -moz-box-shadow: 0 1px 5px #333;
            -webkit-box-shadow: 0 1px 5px #333;
            z-index:101;
        }
.box h1, .myBox h1, .displaybox h1, .myScrollBox h1{
            border-bottom: 1px dashed #7F7F7F;
            margin:-20px -20px 0px -20px;
            padding:10px;
            background-color:#008B8B;
            color:#FFF;
            -moz-border-radius:20px 20px 0px 0px;
            -webkit-border-top-left-radius: 20px;
            -webkit-border-top-right-radius: 20px;
            -khtml-border-top-left-radius: 20px;
            -khtml-border-top-right-radius: 20px;
        }
a.boxclose{
            float:right;
            width:26px;
            height:26px;
            background:transparent url(images/cancel.png) repeat top left;
            margin-top:-30px;
            margin-right:-30px;
            cursor:pointer;
        }

.myBox{
            position:fixed;
            top:-400px;
            left:25%;
            right:30%;
            height: 350px;
	    width:700px; 
            padding:20px;
            scroll: auto; 
	    background-color:#282828;
            color:#FFF;
            -moz-border-radius: 20px;
            -webkit-border-radius:20px;
            -khtml-border-radius:20px;
            -moz-box-shadow: 0 1px 5px #333;
            -webkit-box-shadow: 0 1px 5px #333;
            z-index:101;
}
a.scrollboxclose{
            float:right;
            width:26px;
            height:26px;
            background:transparent url(images/cancel.png) repeat top left;
            margin-top:-20px;
            margin-right:-10px;
            cursor:pointer;
        }
.myScrollBox{
            position:fixed;
            top:-400px;
            left:25%;
            right:30%;
            height: 350px;
	    width:700px; 
            padding:20px;
            overflow:scroll;
            scroll: auto; 
	    background-color:#282828;
            color:#FFF;
            -moz-border-radius: 20px;
            -webkit-border-radius:20px;
            -khtml-border-radius:20px;
            -moz-box-shadow: 0 1px 5px #333;
            -webkit-box-shadow: 0 1px 5px #333;
            z-index:101;
}

