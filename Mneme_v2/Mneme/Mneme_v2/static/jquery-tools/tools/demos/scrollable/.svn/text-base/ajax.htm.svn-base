
<%-- :mode=jsp: --%>
<%@ include file="plugins/navigator.htm" %>

<script>
<c:set var="actions">

function loadItems() {
	
	// get handle to the scripting API
	var api = $("div.scrollable").scrollable();
	
	// load items under the item wrapper
	api.getItemWrap().load("ajax-items.htm", function()  {
			
		// when loading is done peform a reload for the changes to take effect. simple.
		api.reload();			
	});
}
</c:set>

${actions}
</script>


<p style="margin:15px 0 0 37px">
	<button type="button" onclick="loadItems()">load items</button>
</p>



